{
  "name": "spinneret",
  "version": "0.0.5",
  "description": "functional web components",
  "main": "./dist/spinneret.umd.cjs",
  "module": "./dist/spinneret.js",
  "keywords": [
    "functional",
    "fp",
    "dom",
    "web",
    "framework",
    "library",
    "spider",
    "spinneret",
    "generic",
    "simple",
    "dumb"
  ],
  "exports": {
    ".": {
      "import": "./dist/spinneret.js",
      "require": "./dist/spinneret.umd.cjs"
    }
  },
  "type": "module",
  "repository": "brekk/spinneret",
  "author": "brekk",
  "license": "MIT",
  "dependencies": {
    "blem": "0.0.9",
    "ramda": "0.29.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.6",
    "@vitest/coverage-v8": "3.0.5",
    "@vitest/ui": "^3.0.5",
    "dotenv-cli": "7.3.0",
    "eslint": "^9.21.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.11.0",
    "husky": "^8.0.0",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "madge": "^8.0.0",
    "nps": "^5.10.0",
    "prettier": "^3.4.2",
    "sass": "^1.84.0",
    "svelte": "^5.19.9",
    "unusual": "^0.0.3",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  },
  "scripts": {
    "dev": "vite --config vite.config.js",
    "build-pages": "vite build --config vite.config.js",
    "preview-pages": "vite preview --config vite.config.js",
    "build-lib": "vite build --config vite.library.config.js",
    "preview-lib": "vite build --config vite.library.config.js",
    "build": "yarn build-pages && yarn build-lib",
    "lint": "eslint --fix .",
    "test": "vitest --coverage src",
    "test-ci": "vitest --run src",
    "care": "yarn lint && yarn test-ci && yarn build",
    "prepare": "husky install",
    "meta-graph": "madge example.js --image spinneret.svg --ts-config jsconfig.json",
    "meta": "yarn meta-graph"
  },
  "packageManager": "yarn@4.5.1"
}
